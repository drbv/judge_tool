.beamer
  %h1="#{@round.round_type.name} #{@round.dance_class.name}"
  %table.table.table-striped
    //- @dance_team_result_list.first(7).each_with_index do |dance_team, index|
    - @dance_team_result_list.each_with_index do |dance_team, index|
      %tr{class: "#{'success' if @dance_round.dance_teams.include?(dance_team)}"}
        %td="#{index+1}."
        %td #{dance_team.startnumber}
        %td=dance_team.full_name
        %td{class: 'red-font'}="#{'TSO malus' if dance_team.tso_malus?(@round)}"
        %td=dance_team.get_final_result(@round)

#beamer_reload